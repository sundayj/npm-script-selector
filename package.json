{
  "name": "npm-script-selector",
  "version": "1.0.4",
  "description": "A command line app for finding and running npm scripts within a project.",
  "main": "index.js",
  "type": "module",
  "scripts": {

    "build": "tsc",
    "test": "jasmine",
    "start": "node ./dist/app.js",
    "devstart": "npm run build && node ./dist/app.js -f package.json"
  },
  "bin": {
    "npmss": "dist/app.js"
  },
  "keywords": [
    "typescript",
    "npm",
    "package",
    "utility",
    "scripts",
    "script runner",
    "dev tool",
    "tool"
  ],
  "author": "Justin L. Sunday",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.1.0",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.11",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.7",
    "@types/inquirer": "^9.0.6",
    "@types/jasmine": "^5.1.1",
    "@types/node": "^20.8.7",
    "jasmine": "^5.1.0",
    "nyc": "^15.1.0",
    "typescript": "^5.2.2"
  }
}
